<h1>Users</h1>
	<table class="table">
	  	<thead>
	  		<tr>
	  			<th>image</th>
	  			<th>name</th>
	  			<th></th>
	  		</tr>
	  	</thead>
	  	<tbody>
	  	<% @users.each do |user| %>
	  		<tr>
	  		<td><%= attachment_image_tag user,:profile_image, size:"30x30" %></td>
	  		<td><%= user.name %></td>
			<td><%= link_to "Show", user_path(user.id) %></td>
	  		</tr>
	  	<% end %>
	  	</tbody>
 	</table>